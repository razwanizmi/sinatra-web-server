<form action="/urls" method="post">
Enter URL:
	<input type ="text" name="long_url">
	<input type="submit" value="Submit">
</form>

<% @url.reverse.each do |url| %>
	<p> Long url: <%= url.long_url %> Short url: <%= "http://localhost:9393/" + url.short_url %> Click counter: <%= url.click_count %> </p>
<% end %>